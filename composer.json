{
  "name": "elvandar/kazetenn",
  "type": "symfony-bundle",
  "license": "GPL-3.0-or-later",
  "description": "CSM library for symfony",
  "authors": [
    {
      "name": "Elvandar Ysalys",
      "email": "elvandar.ysalys@protonmail.com"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=7.4.6",
    "composer/package-versions-deprecated": "1.11.99.4",
    "doctrine/annotations": "^1.0",
    "doctrine/doctrine-bundle": "^2.4",
    "doctrine/doctrine-migrations-bundle": "^3.1",
    "doctrine/orm": "^2.9",
    "phpdocumentor/reflection-docblock": "^5.2",
    "sensio/framework-extra-bundle": "^6.1",
    "stof/doctrine-extensions-bundle": "^1.7",
    "symfony/asset": "5.4.*",
    "symfony/console": "5.4.*",
    "symfony/dotenv": "5.4.*",
    "symfony/expression-language": "5.4.*",
    "symfony/flex": "^1.3.1",
    "symfony/form": "5.4.*",
    "symfony/framework-bundle": "5.4.4",
    "symfony/http-client": "5.4.*",
    "symfony/intl": "5.4.*",
    "symfony/mailer": "5.4.*",
    "symfony/mime": "5.4.*",
    "symfony/monolog-bundle": "^3.1",
    "symfony/notifier": "5.4.*",
    "symfony/process": "5.4.*",
    "symfony/property-access": "5.4.*",
    "symfony/property-info": "5.4.*",
    "symfony/proxy-manager-bridge": "5.4.*",
    "symfony/runtime": "5.4.*",
    "symfony/security-bundle": "5.4.*",
    "symfony/serializer": "5.4.*",
    "symfony/string": "5.4.*",
    "symfony/translation": "5.4.*",
    "symfony/twig-bundle": "^5.4",
    "symfony/uid": "5.4.*",
    "symfony/validator": "5.4.*",
    "symfony/web-link": "5.4.*",
    "symfony/yaml": "5.4.*",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.4",
    "phpunit/phpunit": "^9.5",
    "symfony/browser-kit": "5.4.*",
    "symfony/css-selector": "5.4.*",
    "symfony/debug-bundle": "5.4.*",
    "symfony/maker-bundle": "^1.0",
    "symfony/phpunit-bridge": "^5.4",
    "symfony/stopwatch": "5.4.*",
    "symfony/web-profiler-bundle": "5.4.*",
    "symfony/webpack-encore-bundle": "^1.13"
  },
  "replace": {
    "elvandar/kazetenn_pages": "self.version",
    "elvandar/kazetenn_articles": "self.version",
    "elvandar/kazetenn_core_admin": "self.version"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "App",
      "Kazetenn\\Pages\\": "src/Pages",
      "Kazetenn\\Articles\\": "src/Articles",
      "Kazetenn\\Core\\": "src/Core"
    }
  }
}
