        <title>{% block title %}{{'kazetenn_admin.base_title'|trans}}{% endblock %}</title>

        {% block stylesheets %}
            <link rel="stylesheet" type="text/css" href="{{ asset('bundles/kazetenn/page_style.css') }}">
